  "scripts": {
    "build": "npm run clean && npm rebuild sass && npm-run-all build:library build:reset build:components build:themes && npm run build:types",
    "build-storybook": "build-storybook -o ./build",
    "build:components": "webpack --config webpack.components.config.js",
    "build:library": "webpack --config webpack.config.js",
    "build:reset": "webpack --config webpack.reset.config.js",
    "build:themes": "sass styles/themes/:dist --style=compressed --source-map --embed-sources",
    "build:types": "tsc --emitDeclarationOnly -skipLibCheck",
    "ci-check": "npm-run-all prettier lint stylelint type-check test",
    "clean": "rm -rf dist || true",
    "lint": "ESLINT_CODE_QUALITY_REPORT=eslint-codequality.json eslint --format gitlab --ext .js,.ts,.tsx,.jsx src/ && echo '✅ PASSED Eslint' || (echo '\nPlease fix code formatting by running:\nnpm run lint:fix\n'; exit 1)",
    "lint:file": "eslint --ext .js,.ts,.tsx,.jsx",
    "lint:fix": "eslint --ext .js,.ts,.tsx,.jsx src/ --fix",
    "postbuild": "cp package.json ./dist/package.json && cp -R styles ./dist/styles",
    "precommit": "lint-staged",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.toString().indexOf('git') == -1) throw e}\"",
    "prettier": "prettier --list-different 'src/**/*.{js,ts,tsx,jsx,scss}' && echo '✅ PASSED Prettier' || (echo '\nPlease fix code formatting by running:\nnpm run prettier:fix\n'; exit 1)",
    "prettier:fix": "prettier --write 'src/**/*.{js,ts,tsx,jsx,scss}'",
    "start": "APPENV=dev npm run storybook && node ./server.js",
    "storybook": "start-storybook -p 3000",
    "stylelint": "stylelint '**/*.scss' && echo '✅ PASSED Stylelint' || (echo '\nPlease fix style formatting by running:\nnpm run stylelint:fix\n'; exit 1)",
    "stylelint:check": "stylelint-config-prettier-check",
    "stylelint:ci": "STYLELINT_CODE_QUALITY_REPORT=stylelint-codequality.json stylelint '**/*.scss' --custom-formatter=node_modules/stylelint-formatter-gitlab && echo '✅ PASSED Stylelint' || (echo '\nPlease fix style formatting by running:\nnpm run stylelint:fix\n'; exit 1)",
    "stylelint:fix": "stylelint '**/*.scss' --fix",
    "sync-locally": "node ./cli/sync-locally.js",
    "test": "PORT=1089 jest --coverage",
    "test:ci": "APP_ENV=test node --expose-gc --max-old-space-size=1024 ./node_modules/.bin/jest --runInBand --logHeapUsage --coverage",
    "test:update": "jest --updateSnapshot",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit -skipLibCheck && echo '✅ PASSED type-check'",
    "type-check:watch": "npm run type-check -- --watch"
  },
